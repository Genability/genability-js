    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "mock-fs": "^4.12.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
